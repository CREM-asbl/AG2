#tag Events PopupMenu1
	#tag Event
		Sub Open()
		  dim i,j as integer
		  dim nom as string
		  dim stdfiles(-1) as string
		  
		  
		  stdfiles = app.StdFilesDispo
		  for i=0 to UBound(stdfiles)
		    me.addRow(stdfiles(i))
		    if stdfiles(i)+".std" = config.stdfile then
		      me.listIndex = i
		    end if
		  next
		  me.ListIndex = max(0,me.ListIndex)
		  
		  
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PopupMenu2
	#tag Event
		Sub Open()
		  me.ListIndex = config.stdsize-1
		End Sub
	#tag EndEvent
	#tag Event
		Sub Change()
		  config.stdsize = val(me.Text)
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events OKButton
	#tag Event
		Sub Action()
		  dim i as integer
		  
		  Config.ChargerStdForms(popupmenu1.text+".std")
		  Config.stdsize = val(popupmenu2.text)
		  Close
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events CancelButton
	#tag Event
		Sub Action()
		  close
		End Sub
	#tag EndEvent
