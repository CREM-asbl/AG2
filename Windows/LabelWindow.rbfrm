#tag Events Cancel
	#tag Event
		Sub Action()
		  dim OldLab as Label
		  
		  OldLab = AddLab.OldLabel
		  
		  If OldLab <> nil then
		    Lab.Text = OldLab.Text
		    Lab.col = OldLab.col
		    Lab.size = OldLab.size
		    Lab.Italique = OldLab.Italique
		    Lab.fixe = OldLab.fixe
		    Lab.Correction = OldLab.Correction
		  else
		    Lab.Text =""
		  end if
		  
		  self.close()
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  me.caption = Dico.value("Cancel")
		End Sub
	#tag EndEvent
