#tag Events Text
	#tag Event
		Sub TextChange()
		  dim n as integer
		  dim s as string
		  dim k as Byte
		  
		  s = me.Text
		  n = s.len
		  k = asc(right(s,1))
		  if k < 32 or k = 127 then
		    s = left(s,n-1)
		    me.Text = s
		  end if
		  
		  
		  Lab.Text = me.Text
		  Wnd.Refresh
		  
		End Sub
	#tag EndEvent
	#tag Event
		Function KeyDown(Key As String) As Boolean
		  if asc(Key) = 13 then
		    self.close
		  end if
		End Function
	#tag EndEvent
#tag EndEvents
