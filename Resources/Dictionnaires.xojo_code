	#tag Method, Flags = &h0
		Function identifier(fa as integer, fo as integer) As string
		  Select Case fa
		  case 0
		    select case fo
		    case 0
		      return point
		    case 1
		      return pointon
		    case 2
		      return interpoint
		    end select
		  case 1
		    select case fo
		    case 0, 1, 2
		      return segment
		    case  3,4,5
		      return droite
		    case 6
		      return demidroite
		    case 7
		      return bande
		    case 8
		      return secteur
		    end select
		  case 2
		    select case fo
		    case 0
		      return triangle
		    case 1
		      return triangleiso
		    case 2
		      return triangleequi
		    case 3
		      return trianglerect
		    case 4
		      return trianglerectiso
		    end select
		  case 3
		    select case fo
		    case 0
		      return quadri
		    case 1
		      return trap
		    case 2
		      return TrapRect
		    case 3
		      return trapiso
		    case 4
		      return parall
		    case 5
		      return rect
		    case 6
		      return los
		    case 7
		      return carre
		    end select
		  case 4
		    select case fo
		    case 0
		      return triangleequi
		    case 1
		      return carre
		    case 2
		      return pentareg
		    case 3
		      return hexareg
		    case 4
		      return heptareg
		    case 5
		      return octoreg
		    case 6
		      return enneareg
		    case 7
		      return decareg
		    case 8
		      return hendecareg
		    case 9
		      return dodecareg
		    end select
		  case 5
		    select case fo
		    case 0
		      return cerc
		    case 1
		      Return arc
		    Case 2
		      Return  dsect
		    Case 3
		      Return HalfDsk
		    end select
		  case 6
		    select case fo
		    case 0
		      return triangle
		    case 1
		      return quadri
		    case 2
		      return penta
		    case 3
		      return hexa
		    case 4
		      return hepta
		    case 5
		      return octo
		    case 6
		      return ennea
		    case 7
		      return deca
		    case 8
		      return hendeca
		    case 9
		      return dodeca
		    end select
		  case 7
		    return Lacet
		  end select
		End Function
	#tag EndMethod

