	#tag Method, Flags = &h0
		Function this(st as string) As string
		  
		  if config.langue = "francais" then
		    select case st
		    case "arc"
		      return "cet " + st
		    case "droite", "bande", "ellipse", "hyperbole", "parabole", "demi-droite"
		      return "cette " + st
		    else
		      return "ce " + st
		    end select
		  else
		    return Dico.value("this") +" "+ dico.value(st)
		  end if
		End Function
	#tag EndMethod

